        "fontStyle": ""
      }
    },
    {
      "scope": [
        "meta.attribute.rust",
        "meta.attribute.rust keyword.operator"
      ],
      "settings": {
        "foreground": "#4E57BA",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.type.lifetime.rust"
      ],
      "settings": {
        "foreground": "#C64600",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.name.function.macro"
      ],
      "settings": {
        "foreground": "#4E57BA",
        "fontStyle": ""
